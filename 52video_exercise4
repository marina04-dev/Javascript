function randomInt(min,max) {
  return Math.floor(Math.random() * (max-min) + min);
}

let array = new Array(10);
for (let i=0; i<array.length; i++){
  array[i]=randomInt(0,101);
}
console.log(array);
let sum=0;
for (let j=0; j<array.length; j++) {
    sum += array[j];
    console.log(sum);
}
mean = sum/array.length;
console.log(mean);
