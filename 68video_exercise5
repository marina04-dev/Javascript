let billingDetails = {
    fullname: "Bob Dole",
    addressLine1: "Line 1",
    addressLine2: "Line 2",
    addressLine3: "Line 3",
    print() {
        console.log(this.fullname + "\n");
        for (let i=1; i<=3; i++) {
            console.log(this.["addressLine" + i] + "\n");
        }
    }
}
