// exercise_1
let strings = ["aabbaa","abababab"];
let regexps = [/a.../,/a..bb/];

for (let string of strings) {
    for (let regexp of regexps) {
        console.log(`${regexp} in ${string}: ${string.search(regexp)>=0}`);
    }
}

// exercise_2
let stringy = ["aabbaa","abababab"];
let regexpy = [/a(..)*b/,/a(..)*.b/];

for (let string of stringy) {
    for (let regexp of regexpy) {
        console.log(`${regexp} in ${string}: ${string.search(regexp)>=0}`);
    }
}
